#include <iostream>
using namespace std;
 
int main(){
   int arr[5]={10,20, 43 ,30, 56};
   int sum=0;
   
   for( int i=0;i<5;i++){
       sum += arr[i];
       }
       
       float average = sum / 5;
       cout<<"sum: "<<sum<<endl;
       cout<<"average: "<<average<<endl;
       
    
    return 0;
}


//output
sum: 159
average: 31
